    "@rollup/plugin-node-resolve": "^13.0.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.6.0",
    "jest": "^27.3.1",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^2.59.0",
    "rollup-plugin-babel": "^3.0.7",
