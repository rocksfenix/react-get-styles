{
  "name": "react-get-styles",
  "version": "0.1.0",
  "description": "Helper to get object with styles from string or template string",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "jest",
    "test:w": "jest --watchAll",
    "size": "size-limit",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "6 KB"
    }
  ],
  "keywords": [
    "styles",
    "react",
    "css",
    "rules"
  ],
  "author": "Gerardo Gallegos <puxerop@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.6.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.66.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-url": "^1.4.0",
    "size-limit": "^0.20.0",
    "standard": "^12.0.1",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "camelcase": "^5.0.0"
  },
  "peerDependencies": {},
  "standard": {
    "parser": "babel-eslint"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GerardoGallegos/react-get-styles.git"
  },
  "bugs": {
    "url": "https://github.com/GerardoGallegos/react-get-styles/issues"
  },
  "homepage": "https://github.com/GerardoGallegos/react-get-styles#readme"
